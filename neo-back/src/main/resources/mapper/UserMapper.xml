<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.neo.mapper.UserMapper">
    <select id="findByUserMobile" resultType="com.example.neo.model.User">
      SELECT * FROM user where mobile = #{mobile}
    </select>
    <select id="findByUserId" resultType="com.example.neo.model.User">
      SELECT * FROM user where userId = #{userId}
    </select>
</mapper>
